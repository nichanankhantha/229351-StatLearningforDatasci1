        "# Separate components y_t = S_t + T_t + R_t on the training set\n",
        "result_add_train = seasonal_decompose(train_data['value'], model='additive')\n",
        "\n",
        "# Get the last available trend and residual from the training set\n",
        "last_trend = result_add_train.trend.iloc[-1]\n",
        "last_residual = result_add_train.resid.iloc[-1]\n",
        "\n",
        "# Get the seasonal component for the test set period by repeating the seasonal pattern from the training data\n",
        "seasonal_train = result_add_train.seasonal\n",
        "seasonal_test = seasonal_train.iloc[test_data.index.month - 1].values\n",
        "seasonal_test = pd.Series(seasonal_test, index=test_data.index)\n",
        "\n",
        "# Create the forecasted time series for the test set\n",
        "# Forecast = last_trend + last_residual + seasonal_test\n",
        "forecast_test = last_trend + last_residual + seasonal_test\n",
        "\n",
        "print(\"Forecasted test data:\")\n",
        "display(forecast_test.head())"
